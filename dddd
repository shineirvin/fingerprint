[32m+[m	[32mpublic function datefilter()[m
[32m+[m	[32m{[m
[32m+[m		[32m$datetime = Carbon::now();[m
[32m+[m[32m        if ($datetime->format('l') === 'Monday') {[m
[32m+[m[32m            return $hari = '1';[m
[32m+[m[32m        } else if ($datetime->format('l') === 'Tuesday') {[m
[32m+[m[32m            return $hari = '2';[m
[32m+[m[32m        } else if ($datetime->format('l') === 'Wednesday') {[m
[32m+[m[32m            return $hari = '3';[m
[32m+[m[32m        } else if ($datetime->format('l') === 'Thursday') {[m
[32m+[m[32m            return $hari = '4';[m
[32m+[m[32m        } else if ($datetime->format('l') === 'Friday') {[m
[32m+[m[32m            return $hari = '5';[m
[32m+[m[32m        } else if ($datetime->format('l') === 'Saturday') {[m
[32m+[m[32m            return $hari = '6';[m
[32m+[m[32m        } else if ($datetime->format('l') === 'Sunday') {[m
[32m+[m[32m            return $hari = '7';[m
[32m+[m[32m        }[m
[32m+[m	[32m}[m
