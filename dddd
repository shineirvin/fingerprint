[32m+[m[32m        }[m
[32m+[m[32m        $user = Auth::User();[m
[32m+[m[32m        $destinationPath = 'uploads/'; // upload path[m
[32m+[m[32m        $extension = $request->file('file')->getClientOriginalExtension(); // getting file extension[m
[32m+[m[32m        $fileName = rand(11111, 99999) . '.' . $extension; // renameing image[m
[32m+[m[32m        $upload_success = $request->file('file')->move($destinationPath, $fileName); // uploading file to given path[m
[32m+[m[32m        $user->fhoto = $destinationPath . $fileName;[m
[32m+[m[32m        $user->save();[m
[32m+[m
[32m+[m[32m        flash()->success('Success!', 'New photo has been uploaded');[m
[32m+[m
[32m+[m[32m        return redirect('profile');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Change User Password[m
[32m+[m[32m     */[m
[32m+[m[32m    public function changepassword(ChangePasswordRequest $request)[m
[32m+[m[32m    {[m
[32m+[m[32m        $data = Auth::User();[m
[32m+[m[32m        if ( Hash::check($request->oldpassword, $data->password) ) {[m
[32m+[m[32m            $data->password = Hash::make($request->newpassword);[m
[32m+[m[32m            $data->save();[m
[32m+[m[32m            flash()->success('Success!', 'Your password has been changed Successfuly');[m
[32m+[m
[32m+[m[32m            return redirect('profile');[m
[32m+[m[32m        } else {[m
[32m+[m[32m            return redirect()->back()->withInput()->withErrors(['oldpassword' => 'Password yang anda masukan salah.']);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
